{
  "name": "rule-25-backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "start-watch": "pm2 start ./bin/www --name='rule-25-backend' --watch && pm2 logs rule-25-backend",
    "stop": "pm2 stop ./bin/www",
    "check:lint": "eslint . --ext .js",
    "fix:lint": "eslint . --fix --ext .js",
    "check:prettier": "prettier -c .",
    "fix:prettier": "prettier -c --write .",
    "pg-killall": "heroku pg:killall --app rule-25-backend"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "pug": "2.0.0-beta11",
    "sequelize": "^6.6.5"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.4.1"
  }
}
